{
    "name": "i love study",
    "description": "i love study",
    "version": "1.0",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": true
    },
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "css": [],
          "js": ["content_script.js"]
        }
      ],
    "commands": {
        "studing_now": {
            "suggested_key": {
                "default": "Ctrl+Shift+E",
                "mac": "MacCtrl+Shift+E"
            },
            "description": "studing now"
        }
    },
    "permissions": [
        "storage",
        "declarativeContent",
        "activeTab"
    ]
}